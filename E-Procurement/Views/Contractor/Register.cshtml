        <div class="page-wrapper job-wrapper">
            <div class="content container" style="width:100%">
                <h2>Contractor Registration Request Form</h2>
                <div class="row">
                    <div class="col-xs-12">
                        <h4 class="page-title">Provide your Company details</h4>
                    </div>
                </div>
                <div class="main-wrapper">
                    <div class="row">
                        <div class="content container-fluid">
                            <div class="content">
                                <center><h2>CONTRACTOR REGISTRATION REQUEST</h2></center>
                                <div class="panel-body">
                                    <div id="feedback" style="display: none" data-dismiss="alert"></div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Registered Business Name: <span class="text-danger">*</span></label>
                                                <input type="text" class="form-control" name="supplierbusinessname" id="supplierbusinessname">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Tax Registration (PIN) No: <span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" name="taxregistration" id="taxregistration">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Contact Person (Name):: <span class="text-danger">*</span></label>
                                                <input type="text" class="form-control" name="contactperson" id="contactperson">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Primary Email Address : <span class="text-danger">*</span></label>
                                                <input class="form-control" type="email" name="supplieremailaddress" id="supplieremailaddress">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Mobile Phone No: <span class="text-danger">*</span></label>
                                            <input type="number" class="form-control" name="phonenumber" id="phonenumber">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Terms and Conditions: <span class="text-danger">*</span></label>
                                            <input class="form-control " type="checkbox" name="termsandconditions" id="termsandconditions">
                                        </div>
                                    </div>
                                </div>
                                <center>
                                    <button class="btn btn-primary center-modal btn_supplierregister" id="btn_supplierregister" type="button">Submit Registration</button>
                                </center>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
