@model IEnumerable<E_Procurement.Models.CountriesModel>

    @{
        ViewBag.Title = "PastExperienceCountries";
        Layout = null;
    }
        <select class="form-control select2" id="countriesdropdownCountries" name="countriesdropdownCountries" data_live_search="true">
            <option value="">--Select Country---</option>
            <optgroup label="All Countries List">
                @foreach (var item in Model)
                {
                    <option value="@item.Code">@item.Name</option>
                }
            </optgroup>
        </select>