model IEnumerable<E_Procurement.Models.EntityType>

    @{
        Layout = null;
    }
    <select class="form-control select2" id="dropdownEntityType" name="dropdownEntityType" data_live_search="true">
        <option value="">--Select Entity Type---</option>
        <optgroup label="Entity Type List">
            @foreach (var item in Model)
            {
                <option value="@item.Code">@item.Description</option>
            }
        </optgroup>
    </select>

