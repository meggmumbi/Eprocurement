                            <div class="panel-body">
                                <ul class="nav nav-tabs">
                                    <li class="active" style="background-color:#D3D3D3">
                                        <a href="#tab_1_1" data-toggle="tab"   <h3 class="panel-title" style="color:black">General Details</h3></a>
                                    </li>
                                    @*<li style="background-color:#D3D3D3">
                                            <a href="#tab_1_2" data-toggle="tab"><h3 class="panel-title" style="color:black">Special Group Registration </h3></a>
                                        </li>*@
                                </ul>
                            </div>
                            <div class="tab-content">
                                <div class="tab-pane fade active in" id="tab_1_1" style="width:100%">
                                    @foreach (VendorModel vendor in Model.Vendors)
                                    {
                                        <div class="panel panel-default">
                                            <div class="box box-default">
                                                <div class="box-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Vendor No  <span class="text-danger">*</span></label>
                                                                <input class="form-control" type="text" id="vendorno" name="vendorno" value="@Session["vendorNo"]" readonly>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Vendor Name<span class="text-danger">*</span> </label>
                                                                <input class="form-control" type="text" id="businessname" name="businessname" value="@Session["fullname"]" readonly>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label class="control-label">VAT Registration No  <span class="text-danger">*</span></label>
                                                                <input class="form-control" type="text" id="vatregistration" name="vatregistration" value="@vendor.VAT_Registration_No" readonly>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Business Type<span class="text-danger">*</span></label>
                                                                @Html.Action("BusinessTypes", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Vendor Type <span class="text-danger">*</span></label>
                                                                <select data-live-search="true" class="form-control input-circle select2" id="vendortype" name="vendortype">
                                                                    <option value="@vendor.Vendor_Type">--Choose Type--</option>
                                                                    <option value="0">Local</option>
                                                                    <option value="1">Foreign</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Ownership Type<span class="text-danger">*</span></label>
                                                                <select data-live-search="true" class="form-control input-circle input-circle select2" id="ownershiptype" name="ownershiptype">
                                                                    <option value="@vendor.Owner_Type">--Choose Type--</option>
                                                                    <option value="1">Sole Ownership.Partnership</option>
                                                                    <option value="2">Registered Company</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Certificate of Incorporation No: <span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" required="required" value="@vendor.CertofIncorporation">
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Incorporation Date: <span class="text-danger">*</span></label>
                                                                <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="dateofincoperation" name="dateofincoperation" required="required"></div>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Operations Start Date:</label>
                                                                <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="startdateoperations" name="startdateoperations" required="required"></div>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Language Code:</label>
                                                                @Html.Action("LanguageCode", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Company Headquarters:</label>
                                                                @Html.Action("RegionsList", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Do you have an AGPO Certificate <span class="text-danger">*</span></label>
                                                                <select data-live-search="true" class="form-control input-circle select2" id="disabled" name="disabled" onchange="showDiv('hidden_div', this)">
                                                                    <option value="0">No</option>
                                                                    <option value="1">Yes</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        @*<div class="col-sm-6">
                                                                <div class="form-group">
                                                                    <label class="control-label">Vision:<span>(Max Characters: 250)</span></label>
                                                                    <input type="text" class="form-control input-circle" id="companyvission" name="companyvission" maxlength="250" required="required" value="@vendor.Vision">

                                                                </div>
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <div class="form-group">
                                                                    <label class="control-label">Mision:<span>(Max Characters: 250)</span></label>
                                                                    <input type="text" class="form-control input-circle" id="companymission" name="companymission" maxlength="250" required="required" value="@vendor.Mision">
                                                                </div>
                                                            </div>*@
                                                    </div>
                                                </div>
                                            </div>
                                            @*<p><strong>NB:</strong>Kindly proceeed to fill the special group registration if you have an AGPO certtificate</p>*@

                                        </div>
                                    }
                                    <center>

                                        <button class="btn btn-primary  saveGeneralDeatils" id="saveGeneralDeatils" name="saveGeneralDeatils" type="button">Save Details</button>
                                    </center>
                                    <button class="btn btn-primary nextBtn  pull-right " type="button">Next</button>


                                    <div id="hidden_div" style="display:none">
                                        <div id="specialgroupfeedback" style="display: none" data-dismiss="alert"></div>
                                        <div class="box box-default">
                                            <p><strong>NB:</strong>This section should only be completed by Special Group Suppliers (Youth, Women and Persons with Disability) who have a valid AGPO (Access to Government Procurement Opportunities) Certificate.</p>

                                            <div class="box-body">
                                                <div class="row">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">AGPO Certificate No  <span class="text-danger">*</span></label>
                                                            <input class="form-control" type="text" id="certificateno" name="certificateno">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Registered Special Group<span class="text-danger">*</span> </label>
                                                            @Html.Action("VendorSpecialGroup", "Home")
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Products/Service Category <span class="text-danger">*</span></label>
                                                            <!--<input class="form-control" type="text" id="servicecategory" name="servicecategory">-->
                                                            @Html.Action("supplierCategories", "Home")
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Certificate Effective Date<span class="text-danger">*</span></label>
                                                            <div class="cal-icon">
                                                                <input class="form-control datetimepicker" type="text" id="effectivedates" name="effectivedates">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Certificate Expiry Date <span class="text-danger">*</span></label>
                                                            <div class="cal-icon">
                                                                <input class="form-control datetimepicker" type="text" id="expirydates" name="expirydates">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <p><strong>NB:</strong> Verification of the validity of your AGPO Certificate can be done by accessing the AGPO website, by clicking on the following link: <a href="https://agpo.go.ke/">AGPO Website</a> </p>

                                            </div>
                                            <center>
                                                <button class="btn btn-primary btn_supplierSpecial_GroupRegister" id="btn_supplierSpecial_GroupRegister" name="btn_supplierSpecial_GroupRegister" type="button">Save Details</button>
                                                <button class="btn btn-primary nextBtn pull-right" type="button" id="nextBtn" name="nextBtn" style="display:none">Continue</button>

                                            </center>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </section>
        </div>
        <div class="tab-content">
            <div class="panel-primary setup-content" id="step-2" style="width:100%">
                <section class="content">
                    <div class="panel panel-primary" style="width:100%">
                        <div class="panel-heading">
                            Communication Details
                            <span class="pull-right"><i class="fa fa-chevron-left"></i>Step 2 of 10 <i class="fa fa-chevron-right"></i></span><span class="clearfix"></span>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="panel-body style="width:100%">
                                    @foreach (VendorModel vendor in Model.Vendors)
                                    {

                                        <div class="panel panel-default">
                                            <div class="box box-default">
                                                <div class="box-body">
                                                    <div class="row">
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">P.O Box/Address: <span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="poboxaddress" name="poboxaddress" value="@vendor.PoBox">
                                                            </div>
                                                        </div>

                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Postal Code:<span class="text-danger">*</span></label>
                                                                @Html.Action("PostalCodeList", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">

                                                            <label class="control-label">Physical Location / Address 2 :</label>
                                                            @*<input class="form-control " type="text" id="physicallocation" name="physicallocation" value="@vendor.Physical_location">*@

                                                            <div class="form-group">
                                                                <label class="control-label">Building /House No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="housenumber" name="housenumber" value="@vendor.HouseNo" />
                                                            </div>

                                                            <div class="form-group">
                                                                <label class="control-label">Floor No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="floornumber" name="floornumber" value="@vendor.FloorNo" />
                                                            </div>


                                                            <div class="form-group">
                                                                <label class="control-label">Plot No:</label>
                                                                <input type="text" class="form-control" id="plotnumber" name="plotnumber" value="@vendor.PlotNo" />
                                                            </div>

                                                            <div class="form-group">
                                                                <label class="control-label">Street / Road:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="streetnumber" name="streetnumber" value="@vendor.StreetorRoad" />
                                                            </div>

                                                        </div>

                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">City:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="postcity" name="postcity" value="@vendor.City" readonly />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Country:<span class="text-danger">*</span></label>
                                                                @Html.Action("CountriesList", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Website URL:</label>
                                                                <input type="text" class="form-control" id="weburl" name="weburl" value="@vendor.WebUrl" />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Tel No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="telephonenumber" name="telephonenumber" value="@Session["PhoneNumber"]" readonly />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Email No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="faxnumber" name="faxnumber" value="@Session["email"]" readonly />
                                                            </div>
                                                        </div>

                                                        <button class="btn btn-primary prevBtn pull-left" type="button">Previous</button>
                                                        <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
        <div class="panel-primary setup-content" id="step-3">
            <div class="panel-heading" style="background-color:#006400">
                <h5 class="panel-title">Business Profile</h5>
            </div>
            <div id="feedback" style="display: none" data-dismiss="alert"></div>
            <div class="panel-body">
                @foreach (VendorModel vendor in Model.Vendors)
            {
                    <div class="panel panel-default">
                        <div class="box box-default">
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label"> Company size Code: <span class="text-danger">*</span></label>
                                            @Html.Action("CompanySize", "Home")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Industry Group: : <span class="text-danger">*</span></label>
                                            @*@Html.Action("IndustryGroups", "Home")*@
                                            @Html.Action("supplierCategories", "Home")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Nominal Capital(Kes):<span class="text-danger">*</span></label>
                                            <input type="number" class="form-control" id="nominalcapital" name="nominalcapital" value="@vendor.NominalCap" />
                                        </div>
                                    </div>
                                    @*<div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Dealership Type:<span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="txtDealertype" name="txtDealertype">
                                                    <option value="">--Select Dealer Type--</option>
                                                    <option value="1">Manufacturer</option>
                                                    <option value="2">Distributor</option>
                                                    <option value="3">Partner</option>
                                                    <option value="4">Reseller</option>
                                                    <option value="5">Other</option>
                                                </select>
                                            </div>
                                        </div>*@
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Maximum Value of Business you can handle at one time(Kes):<span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="maxibizvalues" name="maxibizvalues" value="@vendor.MaxBizValue" />
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Mobile No:<span class="text-danger">*</span></label>
                                            <input type="number" class="form-control" id="mobilenumber" name="mobilenumber" value="@vendor.Phone_No" />
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Nature of Business:<span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="natureofbusiness" name="natureofbusiness" value="@vendor.NatureofBz" />
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <center>
                            <button class="btn btn-primary btn_suppliercompleteregister" id="btn_suppliercompleteregister" type="button">Save Details</button>
                            <button class="btn btn-primary nextBtn pull-right" type="button" id="nextBtn" name="nextBtn">Next</button>
                        </center>

                    </div>
                }

            </div>
        </div>
        <div class="panel-primary setup-content" id="step-4">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    Bank Details
                    <span class="pull-right"><i class="fa fa-chevron-left"></i>Step 2 of 3 <i class="fa fa-chevron-right"></i></span><span class="clearfix"></span>

                </div><br />
                <div class="col-xs-4">

                </div>
                <div class="col-xs-8 text-right m-b-30">
                    <a href="#" class="btn btn-primary pull-right rounded" data-toggle="modal" data-target="#add_banks"><i class="fa fa-plus"></i> Add Banks</a>
                </div>
                <h5>Please provide the details of your bank accounts in the section below</h5>
                <div id="banksfeedback" style="display: none" data-dismiss="alert"></div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table class="table table-striped custom-table datatable" id="tbl_vendor_banks">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th> Bank Code </th>
                                        <th> Bank Name </th>
                                        <th> Bank Account No </th>
                                        <th> Bank Branch No </th>
                                        <th> Bank Branch Name </th>
                                        <th> Currency </th>
                                        <th> Edit </th>
                                        <th> Delete </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (BankModel banks in Model.BanksDetails)
                                    {
                                        bankscounter++;
                                        <tr>
                                            <td>
                                                @bankscounter
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => banks.BankCode)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => banks.BankName)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => banks.BankAccountNo)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => banks.Bank_Branch_No)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => banks.bankBranchName)
                                            </td>

                                            <td>
                                                @Html.DisplayFor(modelItem => banks.CurrencyCode)
                                            </td>
                                            <td>
                                                <a class="btn btn-success" onclick="EditBankDetails('@banks.BankCode','@banks.BankName','@banks.CountryCode','@banks.BankAccountNo','@banks.CurrencyCode','@banks.Bank_Branch_No','@banks.Phone_No','@banks.Post_Code')"><i class="fa fa-pencil m-r-10"></i>Edit</a>

