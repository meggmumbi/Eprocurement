@using E_Procurement.Models
@model dynamic
@{
    ViewBag.Title = "RFIResponseForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int goodsServicesrecords = 0;
    int Servicesrecords = 0;
    int Worksrecords = 0;
    int documentsRecords = 0;
    int attacheddocumentsCounter = 0;
    int tenderCounter = 0;
    int bankscounter = 0;
    int directorCounters = 0;
    int litigationcounter = 0;
    int balancesheet = 0;
    int staffcounter = 0;
    int documentsCounter = 0;
    int RequireddocumentsCounter = 0;
    int pastexperiencecounter = 0;
}
<div class="content container-fluid">
    <div class="row">
        <div class="card">
            <div class="card-header text-center" data-background-color="darkgreen">
                <h6><i><strong>e-Prequalification Response</strong></i></h6>
            </div><br />
        </div>
    </div>

    @foreach (IFPRequestsModel invitation in Model.Response)
    {
        <div class="row">
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Supplier Prequalification </a></li>
                    <li class="breadcrumb-item active">Prequalification No= @invitation.RFI_Documents_No </li>
                    <li class="breadcrumb-item active"> Response No = @invitation.PrequalificationNo</li>

                </ol>
            </div>
        </div>
    }
    <div class="container">
        <div class="stepwizard">
            <div class="stepwizard-row setup-panel">
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-1" type="button" class="btn btn-success btn-circle" data-toggle="tab" data-step="1"><Strong>1</Strong></a>
                    <p><small>Communication Details</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-2" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="2" disabled="disabled"><Strong>2</Strong></a>
                    <p><small>Business Profile</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-3" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="3" disabled="disabled"><Strong>3</Strong></a>
                    <p><small>Shareholders Details</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-4" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="4" disabled="disabled"><Strong>4</Strong></a>
                    <p><small>Beneficiary Details</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-5" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="5" disabled="disabled"><Strong>5</Strong></a>
                    <p><small>Litigation History</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-6" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="6" disabled="disabled"><Strong>6</Strong></a>
                    <p><small>Past Experience</small></p>
                </div>
                @*<div class="stepwizard-step col-xs-1">
                        <a href="#step-6" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="6" disabled="disabled"><Strong>6</Strong></a>
                        <p><small>Audited Financial Statements</small></p>
                    </div>*@
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-7" type="button" class="btn btn-default btn-circle" data-toggle="tab" data-step="7" disabled="disabled"><Strong>7</Strong></a>
                    <p><small>Key Staff/Personnel</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-8" type="button" class="btn btn-default btn-circle" data-step="8" disabled="disabled">8</a>
                    <p><small>Prequalification Response (Section A)</small></p>
                </div>
                <div class="stepwizard-step col-xs-1">
                    <a href="#step-9" type="button" class="btn btn-default btn-circle" data-step="9" disabled="disabled">9</a>
                    <p><small>Prequalification Response (Section B) </small></p>
                </div>
                @*<div class="stepwizard-step col-xs-1">
                        <a href="#step-10" type="button" class="btn btn-default btn-circle" data-step="10" disabled="disabled">10</a>
                        <p><small>Document Attachment</small></p>
                    </div>*@
            </div>
        </div>
    </div>
  


    <div class="panel-body" style="width:100%">
        <div class="tab-content">
            <div class="panel-primary setup-content" id="step-1" style="width:100%">
                <section class="content">
                    <div class="panel panel-primary" style="width:100%">
                        <div class="panel-heading">
                            Communication Details
                            <span class="pull-right"><i class="fa fa-chevron-left"></i>Step 1 of 8 <i class="fa fa-chevron-right"></i></span><span class="clearfix"></span>
                        </div>
                        <div class="panel-body">

                            <div class="row">
                                <div class="panel-body style="width:100%">
                                    @foreach (VendorModel vendor in Model.Vendors)
                                    {

                                        <div class="panel panel-default">
                                            <div class="box box-default">
                                                <div class="box-body">
                                                    <div class="row">
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">P.O Box/Address: <span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="poboxaddress" name="poboxaddress" value="@vendor.PoBox">
                                                            </div>
                                                        </div>

                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Postal Code:<span class="text-danger">*</span></label>
                                                                @Html.Action("PostalCodeList", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">

                                                            <label class="control-label">Physical Location / Address 2 :</label>
                                                            @*<input class="form-control " type="text" id="physicallocation" name="physicallocation" value="@vendor.Physical_location">*@

                                                            <div class="form-group">
                                                                <label class="control-label">Building /House No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="housenumber" name="housenumber" value="@vendor.HouseNo" />
                                                            </div>

                                                            <div class="form-group">
                                                                <label class="control-label">Floor No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="floornumber" name="floornumber" value="@vendor.FloorNo" />
                                                            </div>


                                                            <div class="form-group">
                                                                <label class="control-label">Plot No:</label>
                                                                <input type="text" class="form-control" id="plotnumber" name="plotnumber" value="@vendor.PlotNo" />
                                                            </div>

                                                            <div class="form-group">
                                                                <label class="control-label">Street / Road:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="streetnumber" name="streetnumber" value="@vendor.StreetorRoad" />
                                                            </div>

                                                        </div>

                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">City:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="postcity" name="postcity" value="@vendor.City" readonly />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Country:<span class="text-danger">*</span></label>
                                                                @Html.Action("CountriesList", "Home")
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Website URL:</label>
                                                                <input type="text" class="form-control" id="weburl" name="weburl" value="@vendor.WebUrl" />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Tel No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="telephonenumber" name="telephonenumber" value="@Session["PhoneNumber"]" readonly />
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label class="control-label">Email No:<span class="text-danger">*</span></label>
                                                                <input type="text" class="form-control" id="faxnumber" name="faxnumber" value="@Session["email"]" readonly />
                                                            </div>
                                                        </div>
                                                        <center>
                                                            <button class="btn btn-primary saveCommunicationDetails" id="saveCommunicationDetails" name="saveCommunicationDetails" type="button">Save Details</button>
                                                        </center>
                                                        <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
        <div class="panel-primary setup-content" id="step-2">
            <div class="panel-heading" style="background-color:#006400">
                <h5 class="panel-title">Business Profile</h5>
                <span class="pull-right"><i class="fa fa-chevron-left"></i>Step 2 of 8 <i class="fa fa-chevron-right"></i></span><span class="clearfix"></span>
            </div>
            <div id="feedback" style="display: none" data-dismiss="alert"></div>
            <div class="panel-body">
                @foreach (VendorModel vendor in Model.Vendors)
            {
                    <div class="panel panel-default">
                        <div class="box box-default">
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label"> Company size: <span class="text-danger">*</span></label>
                                            @Html.Action("CompanySize", "Home")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Industry Group: : <span class="text-danger">*</span></label>
                                            @*@Html.Action("IndustryGroups", "Home")*@
                                            @Html.Action("supplierCategories", "Home")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Nominal Capital(Kes):<span class="text-danger">*</span></label>
                                            <input type="number" class="form-control" id="nominalcapital" name="nominalcapital" value="@vendor.NominalCap" />
                                        </div>
                                    </div>
                                    @*<div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Dealership Type:<span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="txtDealertype" name="txtDealertype">
                                                    <option value="">--Select Dealer Type--</option>
                                                    <option value="1">Manufacturer</option>
                                                    <option value="2">Distributor</option>
                                                    <option value="3">Partner</option>
                                                    <option value="4">Reseller</option>
                                                    <option value="5">Other</option>
                                                </select>
                                            </div>
                                        </div>*@
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Maximum Value of Business you can handle at one time(Kes):<span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="maxibizvalues" name="maxibizvalues" value="@vendor.MaxBizValue" />
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Mobile No:<span class="text-danger">*</span></label>
                                            <input type="number" class="form-control" id="mobilenumber" name="mobilenumber" value="@vendor.Phone_No" />
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Nature of Business:<span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="natureofbusiness" name="natureofbusiness" value="@vendor.NatureofBz" />
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <center>
                            <button class="btn btn-primary btn_suppliercompleteregister" id="btn_suppliercompleteregister" type="button">Save Details</button>
                            <button class="btn btn-primary nextBtn pull-right" type="button" id="nextBtn" name="nextBtn">Next</button>
                        </center>

                    </div>
                }

            </div>
        </div>
        <div class="panel-primary setup-content" id="step-3">
            <div class="panel-heading">
                Directors/Shareholders Details
                <span class="pull-right"><i class="fa fa-chevron-left"></i>Step 3 of 8 <i class="fa fa-chevron-right"></i></span><span class="clearfix"></span>
            </div>
            <div class="panel panel-default">
                <div class="box box-default">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-4">
                            </div>
                            <div class="col-xs-8 text-right m-b-30">
                                <a href="#" class="btn btn-primary pull-right rounded" data-toggle="modal" data-target="#add_shareholders"><i class="fa fa-plus"></i> Add Shareholder</a>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div id="shareholdersfeedback" style="display: none" data-dismiss="alert"></div>
                                    <h5>Please provide the complete list of Directors/shareholders. Total ownership must be 100% before submission.</h5>
                                    <div class="table-responsive">
                                        <table class="table table-striped custom-table datatable" id="tbl_shareholder_Details" style="width:96%">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th> Director Code </th>
                                                    <th> Director Name </th>
                                                    <th> Nationality</th>
                                                    <th> Citizenship </th>
                                                    <th> Ownership (%) </th>
                                                    <th> Phone Number </th>
                                                    <th> Edit </th>
                                                    <th> Delete </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (DirectorModel directors in Model.StakeholdersDetails)
                                                {
                                                    directorCounters++;
                                                    <tr>
                                                        <td>
                                                            @directorCounters
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.Entry_No)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.Fullname)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.Nationality)
                                                        </td>

                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.CitizenshipType)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.OwnershipPercentage)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.Phonenumber)
                                                        </td>
                                                        <td>
                                                            <a class="btn btn-success" onclick="EditShareholdersDetails('@directors.Fullname','@directors.IdNumber','@directors.Nationality','@directors.CitizenshipType','@directors.OwnershipPercentage','@directors.Phonenumber','@directors.Address','@directors.Email')"><i class="fa fa-pencil m-r-10"></i>Edit</a>

                                                        </td>
                                                        <td>
                                                            <a class="btn btn-danger" onclick="DeleteShareholder('@Url.Action("DeleteShareholdersDetails", "Home", new { shareholderCode = @directors.Entry_No })')"><i class="fa fa-trash m-r-10"></i>Delete</a>

                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <button class="btn btn-primary prevBtn pull-left" type="button">Previous</button>
                    <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                </div>
            </div>
        </div>

        <div id="add_shareholders" class="modal custom-modal fade" role="dialog">
            <div class="modal-dialog" style="width:100%">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <div class="modal-content modal-lg" style="width:96%">
                    <div class="modal-header">
                        <h5 class="modal-title">Add Shareholder Details</h5>

                    </div>
                    <div class="modal-body">
                        <form class="m-b-30">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Shareholder Type <span class="text-danger">*</span></label>
                                        <select data-live-search="true" class="form-control input-circle select2" id="shareholdertype" name="shareholdertype"  onchange="showDiv('hidden_div', this); showDiv2('hidden_div2', this)" >
                                            <option value="">--Select Shareholder Type--</option>
                                            <option value="0">Person</option>
                                            <option value="1">Company</option>
                                        </select>
                                    </div>
                                </div>
                              
                            
                               
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label"> Name : <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="directorname" name="directorname">
                                    </div>
                                </div>
                                <div id="hidden_div" style="display:none">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Company Registration Number<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="registrationNo" name="registrationNo">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Entity Type<span class="text-danger">*</span></label>
                                            @Html.Action("EntityType", "Home")
                                           
                                        </div>
                                    </div>
                                
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">KRA Pin.<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="kraPin" name="kraPin">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Company Type <span class="text-danger">*</span></label>
                                            <select data-live-search="true" class="form-control input-circle select2" id="companytype" name="companytype">
                                                <option value="">--Select Company Type--</option>
                                                <option value="0">Local</option>
                                                <option value="1">Foreign</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div id="hidden_div2" style="display:block">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">ID / Passport Number: <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="idnumbers" name="idnumbers">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Nationallity </label>
                                            <input class="form-control" type="text" id="nationality" name="ownershippercentage">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Citizenship Type <span class="text-danger">*</span></label>
                                            <select data-live-search="true" class="form-control input-circle select2" id="citizenship" name="citizenship">
                                                <option value="">--Select Citizenship Type--</option>
                                                <option value="1">Birth</option>
                                                <option value="2">Naturalization</option>
                                                <option value="3">Registration</option>
                                            </select>
                                        </div>
                                    </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Ownership Percentage(%) </label>
                                            <input class="form-control" type="text" id="ownershippercentage" name="ownershippercentage">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Phone Number: <span class="text-danger">*</span></label>
                                            <input type="number" class="form-control" id="phonenumbers" name="phonenumbers">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Address: <span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="shareholderaddress" name="shareholderaddress">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Email Address: <span class="text-danger">*</span></label>
                                            <input type="email" class="form-control" id="emailaddress" name="emailaddress">
                                        </div>
                                    </div>

                                </div>
                            <div class="m-t-20 text-center">
                                <button class="btn btn-primary btn_submitShareholders_Details" id="btn_submitShareholders_Details" name="btn_submitShareholders_Details" type="button">Save Shareholders Details</button>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>
        <div id="delete_shareholders_modal" class="modal custom-modal fade" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content modal-md">
                    <div class="modal-header">
                        <h4 class="modal-title">Delete Shareholders Details</h4>
                    </div>
                    <div class="modal-body card-box">
                        <p>Are you sure want to delete this Shareholder?</p>
                        <div class="m-t-20">
                            <a href="#" class="btn btn-default" data-dismiss="modal">Close</a>
                            <button type="submit" class="btn btn-danger">Delete</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-primary setup-content" id="step-4">
            <div class="panel-heading">
               Beneficiary Details
                <span class="pull-right"><i class="fa fa-chevron-left"></i>Step 4 of 8 <i class="fa fa-chevron-right"></i></span><span class="clearfix"></span>
            </div>
            <div class="panel panel-default">
                <div class="box box-default">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-4">
                            </div>
                            <div class="col-xs-8 text-right m-b-30">
                                <a href="#" class="btn btn-primary pull-right rounded" data-toggle="modal" data-target="#add_beneficiaries"><i class="fa fa-plus"></i> Add Beneficiaries</a>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div id="shareholdersfeedback" style="display: none" data-dismiss="alert"></div>
                                    <h5>Please provide the complete list of Beneficiaries.</h5>
                                    <div class="table-responsive">
                                        <table class="table table-striped custom-table datatable" id="tbl_Beneficiary_Details" style="width:96%">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Beneficiary Name </th>
                                                    <th>Beneficiary Type</th>
                                                    <th>ID/Passport Number</th>
                                                    <th>Phone Number</th>
                                                    <th>Email </th>
                                                    <th>Allocated Shares </th>
                                                    <th> Edit </th>
                                                    <th> Delete </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (BeneficiarryModel directors in Model.Beneficiaries)
                                                {
                                                    directorCounters++;
                                                    <tr>
                                                        <td>
                                                            @directorCounters
                                                        </td>
                                                        @*<td>
                                                            @Html.DisplayFor(modelItem => directors.Entry_No)
                                                        </td>*@
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.Name)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.BeneficiaryType)
                                                        </td>

                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.idpassportNumber)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => directors.Phonenumber)
