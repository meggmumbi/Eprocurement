            <div class="tab-pane fade " id="tab_tender_documents">
                <div class="row">
                    <div class="panel panel-heading" style="background-color:#D3D3D3">
                        <h6 class="panel-title"><strong><u>Tender Required Documents</u></strong></h6>
                    </div>
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table class="table table-striped custom-table datatable">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Document Type ID</th>
                                        <th>Description</th>
                                        <th>Requirement Type</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (IfsDocumentTModel item in Model.TenderDocument)
                                    {
                                        tenderDocuments++;
                                        <tr>
                                            <td>
                                                @tenderDocuments
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Procurement_Document_Type_ID)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Description)

                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Requirement_Type)

                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

