}

<div class="content">
    <div class="card-box tab-box">
        <div class="row">
            <div class="card">
                <div class="card-header text-center" data-background-color="darkgreen">
                    <h5 class="title"><i>Tender Response Form</i></h5>
                </div>
            </div>
        </div>
        @foreach (BidResponseDetailsModel item in Model.BidDetails)
