                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Bid Charge Amount(LCY):</label>
                                        <input class="form-control " type="number" id="startdateoperations" name="startdateoperations" value="@item.Bid_Charge_LCY" readonly>
                                    </div>
                                </div>
                             
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Bidder Witness Address:</label>
                                        <input class="form-control " type="text" id="bidderwitnessaddress" name="bidderwitnessaddress" value="@item.Bidder_witness_Address" required>
                                    </div>
                                </div>
                             
                            
