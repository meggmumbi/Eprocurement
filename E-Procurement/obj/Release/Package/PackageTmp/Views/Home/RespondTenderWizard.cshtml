                                        <label class="control-label">Bidder Witness Designation:<span class="text-danger">*</span></label>
                                        <input class="form-control " type="text" id="bidderwitdesignation" name="bidderwitdesignation" value="@item.Bidder_Witness_Name" required>
                                    </div>
