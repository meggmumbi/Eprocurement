                                                            <th> Edit </th>
                                                            <th> Delete</th>
                                                        </tr>

                                                    </thead>
                                                    <tbody>
                                                        @foreach (BidEquipmentsSpecificationModel item in Model.BidEquipments)
                                                                {
                                                                    equipmentsCounter++;
                                                            <tr>
                                                                <td>
                                                                    @equipmentsCounter
