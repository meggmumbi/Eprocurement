                                                <input type="text" class="form-control" id="personnelphonenumber" name="personnelphonenumber" />
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Nationality:<span class="text-danger">*</span></label>
                                                @Html.Action("CountriesList", "Home")
