                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">External Document (Certificate No): <span class="text-danger">*</span></label>
                                                            <input class="form-control " type="text">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Issue Date:<span class="text-danger">*</span></label>
                                                            <input type="text" class="form-control" placeholder="Enter Company Address" />
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Expiry Date:<span class="text-danger">*</span></label>
                                                            <input type="text" class="form-control" placeholder="Enter Company Address" />
                                                        </div>
                                                    </div>

                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Select Document:<span class="text-danger">*</span></label>
                                                            <input type="file" class="form-control" id="inputFileselector" name="inputFileselector[]" placeholder="Enter Company Address" />
                                                        </div>
                                                    </div>
                                                    <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                                </div>
                                            </div>

                                            <div class="tab-pane fade " id="tab_1_attached">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="table-responsive">
                                                            <table class="table table-striped custom-table datatable">
                                                                <thead>
                                                                    <tr>
                                                                        <th>#</th>
                                                                        <th>Description</th>
                                                                        <th>Date Filed</th>
                                                                        <th>Issue Date</th>
                                                                        <th>Expiry Date</th>
                                                                        <th>File Name</th>
                                                                        <th>Download</th>
                                                                        <th>Delete</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    @foreach (AttachedBidDocuments item in Model.AttachedBiddDocuments)
                                                                    {
                                                                        documentsCounter++;
                                                                        <tr>
                                                                            <td>
                                                                                @documentsCounter
                                                                            </td>
                                                                            <td>
                                                                                @Html.DisplayFor(modelItem => item.Description)
                                                                            </td>
                                                                            <td>
                                                                                @Html.DisplayFor(modelItem => item.Date_Filed)
                                                                            </td>
                                                                            <td>
                                                                                @Html.DisplayFor(modelItem => item.Issue_Date)
                                                                            </td>
                                                                            <td>
                                                                                @Html.DisplayFor(modelItem => item.Expiry_Date)
                                                                            </td>
                                                                            <td>
                                                                                @Html.DisplayFor(modelItem => item.File_Name)
                                                                            </td>
                                                                            <td>
                                                                                <a class="btn btn-success" onclick="DownloadDocument('@Url.Action("DownloadRegDocumentsfromSharepoint", "Home", new { filename = @item.File_Name })')"><i class="fa fa-download m-r-10"></i>Download</a>

                                                                            </td>
                                                                            <td>
                                                                                <a class="btn btn-danger" onclick="DocumentsDelete('@Url.Action("DeleteRegDocfromSharepoint", "Home", new { filename = @item.File_Name })')"><i class="fa fa-delete m-r-10"></i>Delete</a>

                                                                            </td>
                                                                        </tr>
                                                                    }

                                                                </tbody>

                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                                <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
                                            </div>

                                        </div>
                                    </div>

                                </div>
</form>

    </div>
