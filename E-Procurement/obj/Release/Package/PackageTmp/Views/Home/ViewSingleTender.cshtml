@using E_Procurement.Models
@model dynamic
@{
    ViewBag.Title = "ViewSingleTender";
    Layout = "";
    int addendumcounter = 0;
    int tenderDocuments = 0;
    int purchaseLines = 0;
    int AllstaffCounter = 0;
    int equipmentsCounter = 0;
    int evaluationsCunter = 0;
    int SecurityCounter = 0;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/img/favicon.png">
    <title>E-Procurement</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/dataTables.bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/CustomCss.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/Custom_tabs_1.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
</head>
<body>
    <div class="main-wrapper">
        @Html.Action("NavigationMenu", "Home")
        <div class="page-wrapper job-wrapper">
            <div class="container" style="width:100%">
                <div class="row">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="~/assets/img/tenders.jpg" width="1400" height="200">
                        </div>
                    </div>
                </div>
                @foreach (SingleTenderModel tenderdetail in Model.SingleTenders)
                {
                    <div class="row">
                        <div class="col-xs-12">
                            <h3 class="page-title"><strong><u>Tender Reference No:</u></strong>@tenderdetail.External_Document_No</h3>
                        </div>
                        <div class="row pull-right">
                            <div class="col-xs-8 text-right m-b-30">
                                <a href="@Url.Action("Login", "Home")" class="btn btn-primary pull-right rounded"><i class="fa fa-plus"></i>Apply for this Tender</a>

                            </div>
                        </div>
                    </div>
                }
            <div class="content" style="width:100%">
                <div class="panel-body">
                    <ul class="nav nav-tabs">
                        <li class="active" style="background-color:#D3D3D3">
                            <a href="#tab_overview" data-toggle="tab"><p style="color:black"><strong>General Tender Overview</strong> </p></a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_purchase_items" data-toggle="tab"><p style="color:black"><strong>Purchase Items/Services</strong></p> </a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_tender_requirements" data-toggle="tab"><p style="color:black"><strong>Tender Requirements</strong></p> </a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_project_works" data-toggle="tab"><p style="color:black"><strong>Tender Project Works</strong></p> </a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_evaluation_creteria" data-toggle="tab"><p style="color:black"><strong>Tender Evaluation Criteria</strong></p> </a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_addendums" data-toggle="tab"><p style="color:black"><strong>Tender Addendums</strong></p> </a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_tender_documents" data-toggle="tab"><p style="color:black"><strong>All Tender Documents</strong></p> </a>
                        </li>
                        <li style="background-color:#D3D3D3">
                            <a href="#tab_tender_requireddocuments" data-toggle="tab"><p style="color:black"><strong>Required Documents</strong></p> </a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane fade active in" id="tab_overview">
                            @foreach (SingleTenderModel tenderdetail in Model.SingleTenders)
                            {
                                <div class="row">
                                    <div class="panel panel-heading" style="background-color:#D3D3D3">
                                        <h6><strong><u>Tender General Details</u></strong></h6>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Tender Description:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="tenderdescription" value="@tenderdetail.Description" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Tender Notice No:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="tendernotice" name="tendernotice" value="@tenderdetail.Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Procurement Method:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="procurementmethod" value="@tenderdetail.Procurement_Method" readonly>
                                        </div>
                                    </div>
                                    @*<div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Mandatory Prebid Meeting:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="mandatorymeeting" value="@tenderdetail.Mandatory_Pre_bid_Visit_Date" readonly>
                                        </div>
                                    </div>*@
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Document Date:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="documentdate" value="@tenderdetail.Document_Date" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Envelope Type:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="enveloptype" value="@tenderdetail.Bid_Envelop_Type" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">External Document No:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="externaldocumentno" value="@tenderdetail.External_Document_No" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Tender Validity Duration(in Days):<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="tendervalidity" value="@tenderdetail.Tender_Validity_Duration" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Location Name:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="locationcode" value="@tenderdetail.Location_Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Tender Validity Expiry Date:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="tenderexpirydate" value="@tenderdetail.Tender_Validity_Expiry_Date" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Requisition Product Group:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="requisitionproductgroup" value="@tenderdetail.Requisition_Product_Group" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Mandatory Special Group Reserve:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="specialgroupreserv" value="@tenderdetail.Mandatory_Special_Group_Reserv" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Target Bidder Group: <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="targetbidergrup" value="@tenderdetail.Target_Bidder_Group" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Selection Method:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="bidselectionmethod" value="@tenderdetail.Bid_Selection_Method" readonly>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="panel panel-heading" style="background-color:#D3D3D3">
                                        <h6 class="panel-title"><strong><u>Critical Submission Details</u></strong></h6>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Submission End Date:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="submissiondate" value="@tenderdetail.Submission_Start_Date" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Address 2:<span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="address" value="@tenderdetail.Address" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Submission End Time: <span class="text-danger">*</span></label>
                                            <input class="form-control " type="text" id="submissiondendtime" value="@tenderdetail.Submission_EndTime" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Postal Code:</label>
                                            <input class="form-control " type="text" id="postcodes" value="@tenderdetail.Post_Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Pre Bid Meeting Date:</label>
                                            <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="startdateoperations" name="startdateoperations" value="@tenderdetail.Mandatory_Pre_bid_Visit_Date" readonly></div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">City:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.City" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Pre-Bid Meeting Address:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Prebid_Meeting_Address" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Country:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Country_Region_Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Opening Date:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Bid_Opening_Date" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Phone Number:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Phone_No" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Opening Time:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Bid_Opening_Time" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Opening Venue:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Bid_Opening_Venue" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Tender Box Location:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Tender_Box_Location_Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Procuring Entity Name:</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Procuring_Entity_Name_Contact" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Primary Tender Submission Address</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Primary_Tender_Submission" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Address</label>
                                            <input type="text" class="form-control" id="cretificateofincorperation" name="cretificateofincorperation" value="@tenderdetail.Primary_Tender_Submission" readonly>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>

                        <div class="tab-pane fade " id="tab_purchase_items">
                            <div class="row">
                                <div class="panel panel-heading" style="background-color:#D3D3D3">
                                    <h6 class="panel-title"><strong><u>Purchase Lines</u></strong></h6>
                                </div>
                                <p>The bidder shall be required to provide pricing information for the Goods and Services listed below. Please note the following:<br /> 1. The bidder must quote for the full quantity on each line.<br /> 2. The Price Quote Model is used to determine how the pricing for each line shall be done.<br /> a) Bidder Pricing-This means that the bidder is allowed to set their best price for each line. There is no restriction on the amount that the bidder can quote in this case.<br /> b) Flat Amount from Buyer-This option is used when the Procuring Entity (The Buyer) has defined a specific standard amount that all bidders shall adopt for a specific purchase line. In this case, the bidder cannot quote more than the set standard amount/limit.</p>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped custom-table datatable">
                                                <thead>
                                                    <tr>
                                                        <th>#</th>
                                                        <th>Type</th>
                                                        <th>Item No.</th>
                                                        <th>Description</th>
                                                        <th>Quantity Required</th>
                                                        <th>Unit of Meassure</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (PurchaseCodeLinesModel item in Model.TenderPurchaseLines)
                                                    {
                                                        purchaseLines++;
                                                        <tr>
                                                            <td>
                                                                @purchaseLines
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Type)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.No)

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Description)

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Quantity)

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Unit_of_Measure_Code)
                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane fade " id="tab_addendums">
                            <div class="row">
                                <div class="panel panel-heading" style="background-color:#D3D3D3">
                                    <h6 class="panel-title"><strong><u>Tender Addendum</u></strong></h6>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-striped custom-table datatable">
                                                <thead>
                                                    <tr>
                                                        <th>#</th>
                                                        <th>Addendum Notice No.</th>
                                                        <th>Addendum Date</th>
                                                        <th>Description</th>
                                                        <th>Addendum Instructions</th>
                                                        <th>View</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (SingleTenderAddendumModel item in Model.ViewTenderAddendums)
                                                    {
                                                        addendumcounter++;
                                                        <tr>
                                                            <td>
                                                                @addendumcounter
                                                            </td>
                                                            <td>
                                                                <a href="@Url.Action("ViewSingleAddendumNotice", "Home", new { AddendumNumber = @item.Addendum_Notice_No })"><strong>@Html.DisplayFor(modelItem => item.Addendum_Notice_No)</strong></a>

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Document_Date)

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Description)

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Addendum_Instructions)

                                                            </td>
                                                            <td>
                                                                <a class="btn btn-success" onclick="@Url.Action("ViewSingleAddendumNotice", "Home", new { AddendumNumber = @item.Addendum_Notice_No }))"><i class="fa fa-eye m-r-10"></i> View</a>

                                                            </td>

                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane fade " id="tab_tender_requirements">
                            <div class="row">
                                <div class="panel panel-heading" style="background-color:#D3D3D3">
                                    <h6 class="panel-title"><strong><u>Bid/Tender Security Requirements</u></strong></h6>
                                </div>
                                <p>The section below lists the accepted forms of Bid/Tender security that the tenderer can submit. Please take note of the validity dates, mimimum value/Amount (In local currency) and the issuing institutions that are acceptable</p>

                                <div class="col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped custom-table datatable">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Forms of Security.</th>
                                                    <th>Security Type</th>
                                                    <th>Nature of Security</th>
                                                    <th>Description</th>
                                                    <th>Minimum Bid Security Amounts(K.E.S)</th>
                                                    <th>Bid Security Validity Expiry Date</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (BidSecurityModel item in Model.TenderSecurity)
                                                {
                                                    SecurityCounter++;
                                                    <tr>
                                                        <td>
                                                            @SecurityCounter
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Form_of_Security)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Security_Type)

                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Nature_of_Security)

                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Description)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Security_Amount_LCY)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.Bid_Security_Validity_Expiry)
                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="panel panel-heading" style="background-color:#D3D3D3">
                                    <h6 class="panel-title"><strong><u>Tender Processing Fees</u></strong></h6>
                                </div>
                                @foreach (SingleTenderModel tenderdetails in Model.SingleTenders)
                                {
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Charge Code:<span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="bidchergecode" value="@tenderdetails.Bid_Charge_Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bank Account Name:<span class="text-danger">*</span> </label>
                                            <input class="form-control" type="text" id="bankaccountname" value="@tenderdetails.Bank_Account_Name" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Charge:<span class="text-danger">*</span> </label>
                                            <input class="form-control" type="text" id="bidcharge" name="bidcharge" value="@tenderdetails.Bid_Charge_LCY" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Charge bank Branch:<span class="text-danger">*</span> </label>
                                            <input class="form-control" type="text" id="bankbranch" name="bankbranch" value="@tenderdetails.Bid_Charge_Bank_Branch" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Charge Bank Code:<span class="text-danger">*</span> </label>
                                            <input class="form-control" type="text" id="bankcode" name="bankcode" value="@tenderdetails.Bid_Charge_Bank_Code" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bid Charge Bank Account No:<span class="text-danger">*</span> </label>
                                            <input class="form-control" type="text" id="acountnumber" name="acountnumber" value="@tenderdetails.Bid_Charge_Bank_A_C_No" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Bank Name:<span class="text-danger">*</span> </label>
                                            <input class="form-control" type="text" id="banknames" name="banknames" value="@tenderdetails.Bank_Name" readonly>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                        <div class="tab-pane fade " id="tab_tender_requireddocuments">
                            <div class="row">
                                <div class="panel panel-heading" style="background-color:#D3D3D3">
                                    <h6 class="panel-title"><strong><u>Tender Required Documents</u></strong></h6>
                                </div>
                                <div class="col-md-12">
                                    <div class="table-responsive">
                                        <table class="table table-striped custom-table datatable">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Document Type ID</th>
                                                    <th>Description</th>
