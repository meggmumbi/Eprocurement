                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label">Total Preliminary (Mandatory Checks Score %):<span class="text-danger">*</span> </label>
                                    <input class="form-control" type="text" id="bankcode" name="bankcode" value="@item.Total_Preliminary_Checks_Score" readonly>
                                </div>
