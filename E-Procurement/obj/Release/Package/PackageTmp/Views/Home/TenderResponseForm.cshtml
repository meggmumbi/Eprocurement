                                <div class="form-group">
                                    <label class="control-label">Total Weighted Score:<span class="text-danger">*</span> </label>
                                    <input class="form-control" type="text" id="acountnumber" name="acountnumber" value="@item.V5_EXCELLENT_Text_Bid_Score" readonly>
                                </div>
                            </div>
