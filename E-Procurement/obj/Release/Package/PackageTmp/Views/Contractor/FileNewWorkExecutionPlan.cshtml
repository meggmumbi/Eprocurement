@using E_Procurement.Models.Contractor
@model dynamic
@{
    ViewBag.Title = "FileNewWorkExecutionPlan";
    Layout = "~/Views/Shared/_ContractorLayout.cshtml";
    int linescounter = 0;
    int Staffcounter = 0;
}
<div class="content">
    <div class="card-box tab-box">
        <center><h2>Project Work Execution Plan</h2></center>
        <div class="row user-tabs">
            <div class="col-lg-12 col-md-12 col-sm-12 line-tabs">
                <div class="stepwizard">
                    <div class="stepwizard-row setup-panel">
                        <div class="stepwizard-step col-xs-2">
                            <a href="#step-1" type="button" class="btn btn-success btn-circle">1</a>
                            <p><small>General Details</small></p>
                        </div>
                        <div class="stepwizard-step col-xs-2">
                            <a href="#step-2" type="button" class="btn btn-default btn-circle" disabled="disabled">2</a>
                            <p><small>Work Execution Schedule</small></p>
                        </div>
                        <div class="stepwizard-step col-xs-2">
                            <a href="#step-3" type="button" class="btn btn-default btn-circle" disabled="disabled">3</a>
                            <p><small>Project Personnel</small></p>
                        </div>
                        <div class="stepwizard-step col-xs-2">
                            <a href="#step-4" type="button" class="btn btn-default btn-circle" disabled="disabled">4</a>
                            <p><small>Equipments Register</small></p>
                        </div>
                        <div class="stepwizard-step col-xs-2">
                            <a href="#step-5" type="button" class="btn btn-default btn-circle" disabled="disabled">5</a>
                            <p><small>Required Documents</small></p>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="modal-body" style="width:100%">
        <form class="m-b-30" role="form">
            <div class="panel-primary setup-content" id="step-1">
                <div class="panel-heading" style="background-color:#006400">
                    <h3 class="panel-title">Project Work Execution Plan General Details</h3>
                </div>
                <div class="tab-content">
                    <div class="tab-pane fade active in" id="tab_1_1">
                        <div id="workexecutionplanfeedback" style="display: none" data-dismiss="alert"></div>
                        @foreach (ProjectWorkExecutionPlanModel item in Model.GeneralDetails)
                        {
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Document No  <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="executionplannumber" name="executionplannumber" value="@item.Document_No" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Commencement Order No  <span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="ordernoticeNumber" name="ordernoticeNumber" value="@item.Commencement_Order_ID" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Country/Region No<span class="text-danger">*</span> </label>
                                        <input class="form-control" type="text" id="contractno" name="contractno" value="@item.Region_ID" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Project No.<span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="ProjectNumber" name="ProjectNumber" value="@item.Project_ID" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Project Title<span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="projecttitle" name="projecttitle" value="@item.Description" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Contractor No<span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="contractorNumber" name="contractorNumber" value="@item.Contractor_No" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Contractor Name<span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="projecttitle" name="projecttitle" value="@item.Contractor_Name" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Road Code<span class="text-danger">*</span></label>
                                        <input class="form-control" type="text" id="roadclass" name="roadclass" value="@item.Road_Code" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Project Start Date<span class="text-danger">*</span></label>
                                        <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="projectstartdate" name="projectstartdate" value="@item.Project_Start_Date" readonly></div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Region:</label>
                                        <input class="form-control " type="text" id="residentengineer" name="residentengineer" value="@item.Region_ID" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Constituency:</label>
                                        <input class="form-control " type="text" id="residentengineer" name="residentengineer" value="@item.Constituency_ID" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Directorate:</label>
                                        <input class="form-control " type="text" id="residentengineer" name="residentengineer" value="@item.Directorate_ID" readonly>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Department:</label>
                                        <input class="form-control " type="text" id="residentengineer" name="residentengineer" value="@item.Department_ID" readonly>
                                    </div>
                                </div>

                               
                            </div>
                                <center>
                                    <button class="btn btn-primary nextBtn pull-right plannextbutton" id="plannextbutton" name="plannextbutton" type="button">Continue</button>
                                </center>
                            }
                    </div>
                </div>
            </div>

            <div class="panel-primary setup-content" id="step-2">
                <div class="panel panel-heading" style="background-color:#006400">
                    <h3 class="panel-title">Project Work Execution  Schedule</h3>
                </div>           
                <div class="row">                  
                    <div class="col-md-12">
                        <div id="workexecutionplanschedulefeedback" style="display: none" data-dismiss="alert"></div>
                        <div class="table-responsive">
                            <table class="table table-striped custom-table datatable" id="tbl_projectschedule">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th> Activity No. </th>
                                        <th> Job Task No.</th>
                                        <th> Description </th>
                                        <th> Duration</th>
                                        <th> Start Date</th>
                                        <th> End Date</th>
                                        <th> Edit </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (WEPExecutionLinesModel item in Model.WEPLines)
                                    {
                                        linescounter++;
                                        <tr>
                                            <td>
                                                @linescounter
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Job_No)

                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Job_Task_No)

                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Description)

                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Budget_Total_Cost)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Scheduled_Start_Date)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Scheduled_End_Date)
                                            </td>
                                            <td>
                                                <a class="btn btn-success" onclick="EditWEPSchedule('@item.Job_No','@item.Job_Task_No','@item.Description')"><i class="fa fa-pencil m-r-10"></i>Edit</a>

                                            </td>

                                        </tr>
                                      }
                                </tbody>
                            </table>
                        </div>
                        <button class="btn btn-primary nextBtn pull-right plannextbutton" id="plannextbutton" name="plannextbutton" type="button">Next</button>

                    </div>
                </div>
            </div>
            <div id="add_executionplanschedule" class="modal custom-modal fade" role="dialog">
                <div class="modal-dialog" style="width:100%">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <div class="modal-content modal-lg" style="width:96%">
                        <div class="modal-header">
                            <h4 class="modal-title">Add Work Execution Plan Schedule</h4>
                        </div>
                        <div class="modal-body">
                            <form class="m-b-30">
                                <div class="row">
                                  @foreach (ProjectWorkExecutionPlanModel item in Model.GeneralDetails)
                                  {
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Document No. <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="wepdocumentnumber" name="wepdocumentnumber" value="@item.Document_No" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Project ID <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="wepprojectnumber" name="wepprojectnumber" value="" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Activity Description <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="wepprojectdescription" name="wepprojectdescription" value="" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Project Task No <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="wepprojecttasknumber" name="wepprojecttasknumber" value="" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Schedule Start Date: <span class="text-danger">*</span></label>
                                            <div class="cal-icon">
                                                <input type="text" class="form-control datetimepicker" id="schedulastartdate" name="schedulastartdate">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Schedule End Date: <span class="text-danger">*</span></label>
                                            <div class="cal-icon">
                                                <input type="text" class="form-control datetimepicker" id="scheduleenddate" name="scheduleenddate">
                                            </div>
                                            </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Duration: <span class="text-danger">*</span></label>
                                            <div class="cal-icon">
                                                <input type="text" class="form-control datetimepicker" id="scheduleenddate" name="scheduleenddate">
                                            </div>
                                        </div>
                                    </div>
                                  }                                   
                                </div>
                                <div class="m-t-20 text-center">
                                    <button class="btn btn-primary btn_submitexecutionplanschedule_Details" id="btn_submitexecutionplanschedule_Details" name="btn_submitexecutionplanschedule_Details" type="button">Work Execution Plan Schedule</button>
                                </div>
                            </form>
                        </div>

                    </div>
                </div>
            </div>
            <div class="panel-primary setup-content" id="step-3">
                <div class="panel-heading" style="background-color:#006400">
                    <h3 class="panel-title">Contractor Project Team</h3>
                </div>              
                <div class="panel-body">
                    <div class="col-xs-8 text-right m-b-30 pull-right">
                        <a href="#" class="btn btn-primary pull-right rounded" data-toggle="modal" data-target="#add_personnel"><i class="fa fa-plus"></i> Add Contractor Personnel</a>
                    </div>
                    <div class="row">                     
                        <div class="row">
                            <div class="col-md-12">
                                <div id="personnelfeedback" style="display: none" data-dismiss="alert"></div>
                                <div class="table-responsive">
                                    <table class="table table-striped custom-table datatable" id="tbl_personel_WEP">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Staff No. </th>
                                                <th>Name</th>
                                                <th>Address </th>
                                                <th>City</th>
                                                <th>Post Code</th>
                                                <th>Country</th>
                                                <th>Edit </th>
                                                <th>Delete </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (WEPContractorTeamModel item in Model.WEPTeam)
                                            {
                                               Staffcounter++;
                                                <tr>
                                                    <td>
                                                        @Staffcounter
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Contractor_Staff_No)

                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Name)

                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Address)

                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.City)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Post_Code)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Country_Region_Code)
                                                    </td>
                                                    <td>
                                                        <a class="btn btn-success" onclick="EditBankDetails('@item.Contractor_Staff_No')"><i class="fa fa-pencil m-r-10"></i>Edit</a>

                                                    </td>
                                                    <td>
                                                        <a class="btn btn-danger" onclick="DeleteBank('@Url.Action("", "", new { items = @item.Contractor_Staff_No })')"><i class="fa fa-delete m-r-10"></i>Delete</a>

                                                    </td>

                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <button class="btn btn-primary nextBtn pull-right plannextbutton" id="plannextbutton" name="plannextbutton" type="button">Next</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="add_personnel" class="modal custom-modal fade" role="dialog">
                <div class="modal-dialog" style="width:100%">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <div class="modal-content modal-lg" style="width:96%">
                        <div class="modal-header">
                            <h4 class="modal-title">Add Contractor Key Personnel</h4>
                        </div>
                        <div class="modal-body">
                            <form class="m-b-30">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Contractor No <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="contractornumber" name="contractornumber" value="@Session["vendorNo"]" readonly>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Contractor Staff No <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="staffnumber" name="staffnumber">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Staff Name</label>
                                            <input class="form-control" type="text" id="staffname" name="staffname">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Staff Email Address: <span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="emailaddress" name="emailaddress">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Address: <span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="staffaddress" name="staffaddress">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Address 2: <span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="staffaddress2" name="staffaddress2">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">City: <span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="staffcity" name="staffcity">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Country/Region: <span class="text-danger">*</span></label>
                                            @Html.Action("ContractorStaffCountryList", "Contractor")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Post Code: <span class="text-danger">*</span></label>
                                            @Html.Action("ContractorPostCodesList", "Contractor")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Mobile Phone Number: <span class="text-danger">*</span></label>
                                            <input type="text" class="form-control" id="mobilenumber" name="mobilenumber">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Role/Designation: <span class="text-danger">*</span></label>
                                            @Html.Action("ProjectStaffRoleCodeList", "Contractor")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Staff Category: <span class="text-danger">*</span></label>
                                            <select data-live-search="true" class="form-control input-circle select2" id="staffcategory" name="staffcategory">
                                                <option value="">--Select Staff Category Type--</option>
                                                <option value="1">Management/Executive Staff</option>
                                                <option value="2">Technical Staff</option>
                                                <option value="3">Consultant</option>
                                                <option value="4">Project Mgt Staff</option>
                                                <option value="5">Site/Delivery Staff</option>
                                                <option value="6">Support Staff</option>
                                                <option value="7">Others</option>
                                            </select>
                                          
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Effective Date: <span class="text-danger">*</span></label>
                                            <div class="cal-icon">
                                                <input type="text" class="form-control datetimepicker" id="staffeffectivedate" name="staffeffectivedate">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="m-t-20 text-center">
                                        <button class="btn btn-primary btn_submitcontractorteam_Details" id="btn_submitcontractorteam_Details" name="btn_submitcontractorteam_Details" type="button">Submit Staff Details</button>
                                    </div>
                                </div>
                            </form>
                        </div>

                    </div>
                </div>
            </div>
            <div class="panel-primary setup-content" id="step-4">
                <div class="panel-heading" style="background-color:#006400">
                    <h3 class="panel-title">Project Equipments Register Details</h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-xs-8 text-right m-b-30 pull-right">
                            <a href="#" class="btn btn-primary pull-right rounded" data-toggle="modal" data-target="#add_equipments"><i class="fa fa-plus"></i> Add Project Equipments</a>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div id="equipementdetailsfeedback" style="display: none" data-dismiss="alert"></div>
                                <div class="table-responsive">
                                    <table class="table table-striped custom-table datatable" id="tbl_projectequipments_contractorregister">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th> Equipment No</th>
                                                <th> Equipment Type</th>
                                                <th> Ownership Type </th>
                                                <th> Equipment Serial No</th>
                                                <th> Equipment Usability </th>
                                                <th> Year of Previous Use</th>
                                                <th> Edit </th>
                                                <th> Delete </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (WEPContractorEquipment item in Model.Equipment)
                                            {
                                                Staffcounter++;
                                                <tr>
                                                    <td>
                                                        @Staffcounter
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Equipment_No)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Equipment_Type_Code)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Ownership_Type)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Equipment_Serial_No)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Equipment_Usability_Code)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Years_of_Previous_Use)
                                                    </td>
                                                    <td>
                                                        <a class="btn btn-success" onclick="EditBankDetails('@item.Equipment_No')"><i class="fa fa-pencil m-r-10"></i>Edit</a>
                                                    </td>
                                                    <td>
                                                        <a class="btn btn-danger" onclick="DeleteBank('@Url.Action("", "", new { items = @item.Equipment_No })')"><i class="fa fa-delete m-r-10"></i>Delete</a>

                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <button class="btn btn-primary nextBtn pull-right plannextbutton" id="plannextbutton" name="plannextbutton" type="button">Next</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
